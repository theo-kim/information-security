if "testfile.txt" in listfiles():
  removefile("testfile.txt")

myfile=openfile("testfile.txt",True)  #Create a parity file
input=(("BBB",0),("AAA",0),("AAAAAAA",0),("BBBBBBB",0),("AAAAAAAB",0),("AAAAAAAA",0),("BB",2),("BB",3),("BBBBBBB",1),("BBB",2),("BBBB",2),("",0),("",-1),("AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA",8),("AAAAAABAAAAA",32),("AABBAAAAA",32),("BAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA",7),("AABBAAAAAAAABBAAAAAAABBAAAAAAABBAAAAA",7),("AABABBAABAAABBBABBBBABABABABBABBAABAABABBAABABBA",44),("",44),("AAAAAAAAAAAAAAAAAAAAA",47),("BAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA",47),("AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAb",47),("AAAAAAAAABBAAAAABBAAAAABBAAAAABBAAABBAABBAABBAABB",47),("aaaaBaBaBaBaaaaBaaaaaaBaBaBaBaaaaBaaa",47))
for (data, offset) in input:
  try:
    myfile.writeat(data,offset)
  except RepyParityError:
    log("Non-even parity write to file for(",data,",",offset,")\n")  # should happen



myfile.readat(None,0)
myfile.readat(None,78)
